    <title>Dashboard Admin</title>
    <!-- Bootstrap core CSS-->
    <link href="../../Admin_Assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom fonts for this template-->
    <link href="../../Admin_Assets/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!-- Page level plugin CSS-->
    <link href="../../Admin_Assets/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="../../Admin_Assets/css/sb-admin.css" rel="stylesheet">

    <link href="../Admin_Assets/css/admin_style.css" rel="stylesheet" />

    <style type="text/css">
        .form-control {
            border: 1px solid #728590;
            padding: 0.175rem .75rem;
            font-size: .9rem;
        }

        select.form-control:not([size]):not([multiple]) {
            height: calc(1.75rem + 2px);
        }

        .form-control:focus {
            border-color: #3c8dbc;
            box-shadow: none;
        }

        .btn-primary {
            color: #fff;
            background-color: #3c8dbc;
            border-color: #3c8dbc;
        }

        .btn {
            padding: .175rem .75rem;
        }
    </style>

    <style type="text/css">
        .input-validation-error {
            border: 1px solid #ff0000 !important;
        }

        .field-validation-error {
            color: #ff0000;
        }
    </style>

</head>
<body class="fixed-nav sticky-footer bg-dark" id="page-top">
